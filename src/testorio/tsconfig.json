{
  "compilerOptions": {
    "target": "esnext",
    "module": "none",
    "moduleResolution": "node",
    "types": ["typed-factorio/runtime", "luassert-tstl"],
    "lib": ["esnext"],
    "plugins": [
      {
        "name": "typescript-tstl-plugin"
      }
    ],
    "strict": true,
    "experimentalDecorators": true,
    "rootDir": "."
  },
  "tstl": {
    "luaTarget": "5.2",
    "sourceMapTraceback": true,
    "noImplicitSelf": true,
    "luaBundle": "../_testorio.lua",
    "luaBundleEntry": "loadTestorio.ts"
  },
  "include": [".", "../../node_modules/typed-factorio/runtime/index.d.ts", "../../index.d.ts"],
  "exclude": []
}
